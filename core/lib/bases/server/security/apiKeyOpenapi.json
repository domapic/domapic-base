{  
  "tags": [{
    "name": "security",
    "description": "Domapic service authentication"
  }],
  "components": {
    "securitySchemes": {
      "apiKey": {
        "type": "apiKey",
        "name": "X-Api-Key",
        "in": "header"
      }
    }
  },
  "paths": {
    "/auth/apikey": {
      "post": {
        "tags": ["security"],
        "summary": "Creates an Api Key",
        "description": "Returns an api key",
        "operationId": "apiKeyAuthenticate",
        "responses": {
          "200": {
            "description": "Api key created"
          }
        }
      },
      "delete": {
        "tags": ["security"],
        "summary": "Remove Api Key",
        "description": "Remove an existant api key",
        "operationId": "apiKeyReject",
        "responses": {
          "200": {
            "description": "Api Key removed"
          }
        }
      }
    }
  }
}
